{
  "name": "sandbox",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:uimix": "uimix compile",
    "build:tsc": "tsc",
    "build:vite": "vite build",
    "build:components": "vite build -c vite.config.components.js",
    "build": "run-z build:uimix build:tsc build:vite build:components",
    "build-storybook": "storybook build",
    "dev:vite": "vite --port 5174",
    "dev:uimix": "uimix compile -w src/uimix/*.uimix",
    "dev:components": "vite --port 5175 -c vite.config.components.js",
    "dev": "run-z dev:vite,dev:uimix,dev:components",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "uimix": "uimix --port 4001"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-mdx-gfm": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/testing-library": "^0.1.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-loader": "^9.1.2",
    "react-test-renderer": "^18.2.0",
    "run-z": "^1.11.0",
    "storybook": "^7.0.2",
    "typescript": "^5.0.3",
    "uimix": "workspace:*",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }
}
