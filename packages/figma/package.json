{
  "name": "@uimix/figma",
  "version": "1.0.0",
  "private": true,
  "description": "Figma plugin for UIMix",
  "license": "MIT",
  "scripts": {
    "build": "npm run tsc && npm run build:ui && npm run build:main",
    "build:main": "esbuild plugin-src/code.ts --target=es6 --bundle --outfile=dist/code.js",
    "build:ui": "npx vite build --minify esbuild --emptyOutDir=false",
    "build:watch": "concurrently -n widget,iframe \"npm run build:main -- --watch\" \"npm run build:ui -- --watch\"",
    "dev": "concurrently -n tsc,build,vite 'npm:tsc:watch' 'npm:build:watch' 'vite'",
    "format": "prettier --write .",
    "lint": "eslint plugin-src ui-src",
    "test": "vitest run -r .",
    "tsc": "npm run tsc:main && npm run tsc:ui",
    "tsc:main": "tsc --noEmit -p plugin-src",
    "tsc:ui": "tsc --noEmit -p ui-src",
    "tsc:watch": "concurrently -n widget,iframe \"npm run tsc:main -- --watch --preserveWatchOutput\" \"npm run tsc:ui -- --watch --preserveWatchOutput\"",
    "package": "ts-node scripts/packageZip.ts"
  },
  "dependencies": {
    "@iconify/react": "^4.1.0",
    "@uimix/model": "workspace:*",
    "buffer": "^6.0.3",
    "js-sha256": "^0.9.0",
    "just-compare": "^2.3.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.12.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.63.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/shelljs": "^0.8.12",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.18",
    "postcss": "^8.4.23",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-singlefile": "^0.13.5",
    "vitest": "^0.30.1",
    "zip-a-folder": "^1.1.5"
  }
}
