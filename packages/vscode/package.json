{
  "name": "macaron-vscode",
  "displayName": "Macaron",
  "version": "0.0.1",
  "private": true,
  "description": "Macaron editor for VS Code",
  "categories": [
    "Visualization",
    "Programming Languages"
  ],
  "icon": "icon.png",
  "publisher": "Macaron",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macaron-elements/macaron.git"
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "scripts": {
    "build": "vsce package && node scripts/copy-vsix-to-docs.js",
    "compile:copy-files": "cp ../editor/dist/vscode/main.js ./out/webview.js",
    "compile:esbuild": "node build.js",
    "dev": "node build.js --dev",
    "lint": "eslint src --ext ts",
    "pretest": "run-s build lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "run-s compile:esbuild compile:copy-files"
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "macaron.macaronFile",
        "displayName": "Macaron",
        "selector": [
          {
            "filenamePattern": "*.macaron"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "html",
        "extensions": [
          ".macaron"
        ]
      }
    ]
  },
  "activationEvents": [
    "onCustomEditor:macaron.macaronFile"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "@types/serve-handler": "^6.1.1",
    "comlink": "^4.3.1",
    "globby": "^13.1.1",
    "lodash": "^4.17.21",
    "serve-handler": "^6.1.3"
  },
  "devDependencies": {
    "@macaron-elements/editor": "*",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "17.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "esbuild": "^0.14.42",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "vsce": "^2.9.1"
  },
  "engines": {
    "vscode": "^1.67.0"
  }
}
